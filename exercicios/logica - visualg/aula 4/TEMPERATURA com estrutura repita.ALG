Algoritmo "sem_nome"

Var

 x, s, c, m : real

Inicio

   escreval("Insira os dados abaixo.")
   escreval("Digite a senha: ")
   leia(x)
   
   enquanto x <> "2312" faca
      escreva("Senha invalida, tente novamente: ")
      leia(x)
   fimenquanto
   
   se x = "2312" entao
      escreval("Acesso Permitido!")
   fimse

Fimalgoritmo