Algoritmo "notas"

Var

 nota1, nota2, notafinal : real

Inicio

      escreva("digite qual foi sua primeira nota: ")
   leia(nota1)
   
      escreva("Digite qual foi sua segunda nota: ")
leia(nota2)
   
      notafinal <- nota1 + nota2
   
      escreval
      escreva("nota final de: ", notafinal)
    se notafinal < 60 entao
       escreval
       escreva("REPROVADO")
    fimse




Fimalgoritmo