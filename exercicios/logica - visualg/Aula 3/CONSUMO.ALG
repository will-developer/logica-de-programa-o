Algoritmo "consumo"

Var

   distancia:real
   combustivel:real
   
Inicio

   escreval("Insira os dados abaixo")
   escreva("Distancia percorrida: ")
   leia(distancia)
   
   escreva("Combustível gasto: ")
   leia(combustivel)
   
   escreva("Consumo médio = ", distancia/combustivel:8:3)

Fimalgoritmo